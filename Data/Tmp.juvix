--- the functions here should be eventually put in the stdlib
module Data.Tmp;

import Stdlib.Prelude open;

printNatListLn : List Nat â†’ IO
  | nil := printStringLn "nil"
  | (x :: xs) :=
    printNat x >> printString " :: " >> printNatListLn xs;

mapMaybe {A B} (f : A -> B) : Maybe A -> Maybe B
  | nothing := nothing
  | (just a) := just (f a);

isJust {A} : Maybe A -> Bool
  | nothing := false
  | (just _) := true;
