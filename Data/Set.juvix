module Data.Set;

import Stdlib.Prelude open;
import Data.Set.AVL open public;

import Stdlib.Trait.Eq as Eq;
open Eq using {Eq};

import Stdlib.Trait.Ord as Ord;
open Ord using {Ord};

Set : Type -> Type;
Set A := AVLTree A;

module SetTraits;
  Eq : {A : Type} -> Eq.Eq A -> Eq.Eq (Set A);
  Eq := AVLTraits.Eq;

  Ord : {A : Type} -> Ord.Ord A -> Ord.Ord (Set A);
  Ord := AVLTraits.Ord;
end;
